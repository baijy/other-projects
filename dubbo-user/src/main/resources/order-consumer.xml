<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans.xsd        http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="order-cosumer" owner="jianyu"/>

    <!--dubbo这个服务所要暴露的服务地址所对应的注册中心-->
    <dubbo:registry protocol="zookeeper" address="192.168.220.128:2181,192.168.220.129:2181,192.168.220.130:2181"/>

    <!-- 直接引入外部jar包 快捷键 Ctrl+Alt+Shift+S —— modules -->
    <dubbo:reference id="orderServices"  interface="com.jianyu.dubbo.order.IOrderServices"/>


</beans>
